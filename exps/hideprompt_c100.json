{
    "prefix": "reproduce",
    "dataset": "cifar224",
    "memory_size": 0,
    "memory_per_class": 0,
    "fixed_memory": false,
    "shuffle": true,
    "init_cls": 10,
    "increment": 10,
    "model_name": "hideprompt",
    "backbone_type": "vit_base_patch16_224_hideprompt",
    "original_model": "vit_base_patch16_224",
    "get_original_backbone": true,
    "device": ["0"],
    "seed": [1993],
    
    "tuned_epoch": 50,
    "init_lr": 0.0005,
    "batch_size": 24,
    "weight_decay": 5e-4,
    "min_lr": 1e-5,
    "optimizer": "adam",
    "scheduler": "step",
    "reinit_optimizer": true,
    "init_milestones": [10],
    "init_lr_decay": 0.1,

    "global_pool": "token",
    "head_type": "token",
    "freeze": ["blocks", "patch_embed", "cls_token", "norm", "pos_embed"],

    "pretrained": true,
    "drop": 0.0,
    "drop_path": 0.0,

    "use_g_prompt": false,
    "g_prompt_length": 5,
    "g_prompt_layer_idx": [],
    "use_prefix_tune_for_g_prompt": false,

    "use_e_prompt": true,
    "e_prompt_layer_idx": [0, 1, 2, 3, 4],
    "use_prefix_tune_for_e_prompt": true,

    "prompt_pool": true,
    "size": 10,
    "length": 20,
    "top_k": 1,
    "initializer": "uniform",
    "prompt_key": false,
    "prompt_key_init": "uniform",
    "use_prompt_mask": true,
    "shared_prompt_pool": true,
    "shared_prompt_key": false,
    "batchwise_prompt": false,
    "embedding_key": "cls",
    "predefined_key": "",
    "same_key_value": false,

    "train_mask": true,
    "larger_prompt_lr": true,
    "prompt_momentum": 0.01,
    "reg": 0.1
}

